{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "routes/api_routes.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "index.js" },
    { "src": "/api", "dest": "routes/api_routes.js" },
    { "src": "/api/tiktok", "dest": "routes/api_routes.js" }
  ]
}

